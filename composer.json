{
    "name": "aidantwoods/secureparsedown",
    "description": "A Parsedown extension, for security.",
    "keywords": [
        "secure", "parsedown", "secureparsedown", "markdown", "parser"
    ],
    "homepage": "https://github.com/aidantwoods/SecureParsedown/wiki",
    "support": {
        "issues": "https://github.com/aidantwoods/SecureParsedown/issues",
        "source": "https://github.com/aidantwoods/SecureParsedown"
    },
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aidan Woods",
            "email": "aidantwoods@gmail.com",
            "homepage": "https://aidanwoods.com"
        }
    ],
    "require": {
        "erusev/parsedown": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Aidantwoods\\SecureParsedown\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload-dev": {
        "psr-4": {
            "Aidantwoods\\SecureParsedown\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": {
            "erusev/parsedown": "source"
        }
    }
}
